<h1>Compose</h1>

<form action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="" name="titleBody">
    </div>

    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="7" name="postBody"></textarea>
    </div>
    <button type="submit" id="compose-button-settings" class="btn btn-secondary" name="button">Publish</button>
</form>

